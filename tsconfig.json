{
  "compilerOptions": {
    "target": "es6", 
    "module": "commonjs",
    "lib": [
      "dom",
      "es5",
      "es6"
    ],
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "strictPropertyInitialization": false,
    "moduleResolution": "node",
    "rootDir": "./src/js/",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
} 
